{
  "name": "baekjoonhub",
  "version": "1.0.0",
  "description": "Automatically sync your code from BOJ/Programmers to GitHub",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
