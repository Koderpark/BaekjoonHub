    "lodash": "^4.17.21",
    "lucide-react": "^0.263.1",
    "mathjs": "^11.11.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "semantic-ui-css": "^2.5.0",
